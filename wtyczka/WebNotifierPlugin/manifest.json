{
  "name": "Web-Notifier",
  "description": "Dodaje strony do obserwowania",
  "version": "1.0",
  "permissions": [
    "downloads",
    "alarms",
    "tabs",
    "webNavigation",
    "<all_urls>"
  ],
  "minimum_chrome_version": "22.0.0",
  "browser_action": {
    "default_title": "Dodawaj elementy",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "manifest_version": 2
}